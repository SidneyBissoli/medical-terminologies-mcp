{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.SidneyBissoli/medical-terminologies-mcp",
  "description": "Unified access to global medical terminologies: ICD-11, SNOMED CT, LOINC, RxNorm, MeSH",
  "repository": {
    "url": "https://github.com/SidneyBissoli/medical-terminologies-mcp",
    "source": "github"
  },
  "version": "1.0.2",
  "packages": [
    {
      "registryType": "npm",
      "identifier": "medical-terminologies-mcp",
      "version": "1.0.2",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "name": "WHO_CLIENT_ID",
          "description": "WHO ICD-11 API Client ID (get from https://icd.who.int/icdapi)",
          "isRequired": true,
          "isSecret": false
        },
        {
          "name": "WHO_CLIENT_SECRET",
          "description": "WHO ICD-11 API Client Secret",
          "isRequired": true,
          "isSecret": true
        }
      ]
    }
  ]
}